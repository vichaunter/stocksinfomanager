{
  "name": "@backend/api",
  "version": "1.0.0",
  "description": "The Stocks info api that everybody should have",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"DEV=true ts-node ./test/dev/index.ts\"",
    "start": "nodemon --ignore data/ --esm ./src/index.ts",
    "services": "cross-env MODE=services ts-node --esm ./src/index.ts",
    "build": "rimraf dist && yarn tsc",
    "ts:check": "yarn tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "sh ./scripts/deploy.sh",
    "db:generate": "yarn prisma generate",
    "db:seed": "node ./database/seed/seed.js",
    "db:update": "node ./database/seed/updateHandlers.js",
    "db:dump": "node ./database/seed/dumpDb.js",
    "db:create": "node ./database/seed/createTickers.js",
    "tools:calendar": "ts-node ./tools/nasdaqcalendar.ts",
    "db:studio": "npx prisma studio"
  },
  "keywords": [
    "stocks",
    "info"
  ],
  "author": "VicHaunter",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@prisma/client": "5.3.1",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.0",
    "csv-parser": "^3.0.0",
    "csv-string": "^4.1.1",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "dotenv": "16.3.1",
    "express": "^4.18.2",
    "fflate": "^0.8.1",
    "fs": "^0.0.1-security",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "onchange": "^7.1.0",
    "picocolors": "^1.0.0",
    "puppeteer": "^21.2.1",
    "puppeteer-core": "^21.2.1",
    "ramda": "^0.29.1",
    "request": "^2.88.2",
    "system-locale": "^0.1.0"
  },
  "devDependencies": {
    "@types/puppeteer": "^7.0.4",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-puppeteer": "^9.0.1",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.0.1",
    "pre-commit": "^1.2.2",
    "prisma": "^5.3.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0-dev.20230917",
    "yargs": "^17.7.2"
  },
  "pre-commit": [
    "ts:check",
    "build",
    "add-build"
  ]
}
